{
  "name": "ringcentral-client",
  "version": "1.0.1",
  "scripts": {
    "prepublish": "npm run build",
    "start": "node server.js",
    "build": "./scripts/build.sh",
    "watch": "npm run build -- watch",
    "test": "npm run build && npm run mocha",
    "mocha": "./node_modules/.bin/mocha ./build/tests/specs.js",
    "istanbul": "./node_modules/.bin/istanbul cover -x specs.js --dir=./build/coverage --report=lcov --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --reporter spec ./build/test/specs.js",
    "generate": "mkdir -p src/generated/url-builders; node codegen/generate.js  --out src/generated codegen/swagger.json",
    "docs": "typedoc --mode file --out ./build/api-docs/ src/ --module es6 -t es6 --exclude **/src/test/**/*.ts --includeDeclarations",
    "lint": "tslint -c tslint.json codegen/ts-sample/*.ts src/**/*.ts"
  },
  "dependencies": {
    "es6-promise": "^3.3.0",
    "form-data": "^1.0.0-rc4",
    "isomorphic-fetch": "^2.2.1",
    "known-fetch-body": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "chalk": "^1.1.3",
    "handlebars": "^4.0.5",
    "istanbul": "^0.4.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "tslint": "^3.13.0",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.0",
    "uppercamelcase": "^1.1.0",
    "webpack": "^1.12.2"
  },
  "preferGlobal": false,
  "private": false,
  "main": "./build/src/Client.js",
  "typings": "./src/Client.ts",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {
      "name": "Kirill Konshin"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/ringcentral-js-client.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-js-client/issues"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-js-client",
  "engines": {
    "node": ">=4.0.0 <6.3.0"
  },
  "license": "MIT"
}
